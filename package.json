{
  "name": "expo-template-reactrouter-nativebase",
  "version": "1.2.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspace @myapp/app run build",
    "lint": "eslint --ext .js,.tsx,.ts . && madge --circular --extensions ts,tsx,js ./",
    "start": "yarn workspace @myapp/app run start",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^29",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.6.2",
    "madge": "^6.1.0",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.2.4",
  "engines": {
    "node": "^20",
    "npm": "please-use-yarn",
    "yarn": ">=3.2"
  }
}
