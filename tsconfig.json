{
  "compilerOptions": {
    "strict": false,
    "target": "es5",
    "module": "esnext",
    "jsx": "react-jsx",

    "allowJs": true,
    "incremental": true,
    "declaration": true,
    "declarationMap": true,

    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
